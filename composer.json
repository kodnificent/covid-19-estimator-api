{
    "name": "kodnificent/covid-19-estimator-php-api",
    "authors": [
        {
            "name": "Victor Mbamara (Kodnificent)",
            "email": "mbamaravictor@gmail.com",
            "homepage": "https://github.com/kodnificent",
            "role": "Developer"
        }
    ],
    "repositories": [
